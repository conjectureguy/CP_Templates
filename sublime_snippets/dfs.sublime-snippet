  <content><![CDATA[
void dfs(ll s, vector<vector<ll>> &adj, vector<bool> &vis){
  if (vis[s])
  {
    return;
  }
  vis[s] = true;
  for(auto u: adj[s]){
    dfs(u, adj, vis);
  }
}
]]></content>
  <tabTrigger>dfs</tabTrigger>
